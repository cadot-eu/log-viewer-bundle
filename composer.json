{
    "name": "evotodi/log-viewer-bundle",
    "description": "Simple log viewer bundle for symfony 5/6",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "evotodi",
            "email": "evotodi@gmail.com"
        }
    ],
    "require": {
        "php": "^8",
        "jdorn/sql-formatter": "^1.2"
    },
    "require-dev": {
        "ext-json": "*",
        "symfony/config": "^5.4|^6.0|^6.1|^6.2",
        "symfony/dependency-injection": "^5.4|^6.0|^6.1|^6.2",
        "symfony/http-kernel": "^5.4|^6.0|^6.1|^6.2",
        "symfony/framework-bundle": "^5.4|^6.0|^6.1|^6.2",
        "symfony/phpunit-bridge": "^5.4|^6.0|^6.1|^6.2",
        "symfony/browser-kit": "^5.4|^6.0|^6.1|^6.2",
        "symfony/twig-bundle": "^5.4|^6.0|^6.1|^6.2",
        "twig/twig": "^2.0 || ^3.0"
    },
    "suggest": {
        "nesbot/carbon": "^2.60.0"
    },
    "autoload": {
        "psr-4": {
            "Evotodi\\LogViewerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Evotodi\\LogViewerBundle\\Tests\\": "tests/"
        }
    }
}
